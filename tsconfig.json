{
    "compilerOptions": {
        "target": "es5",
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "module": "amd",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "removeComments": true,
        "suppressImplicitAnyIndexErrors": true,
        "declaration": true,
        "types": [
            "jasmine",
            "jasmine-ajax"
        ]
    },
    "exclude": [
        "schema"
    ]
}